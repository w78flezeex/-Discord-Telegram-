# Discord Bot Configuration
# Получите токен на https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Telegram Bot Configuration
# Получите токен через @BotFather в Telegram
TELEGRAM_TOKEN=your_telegram_bot_token_here

# ID канала Telegram для отправки сообщений
# Для групп: -100xxxxxxxxxx, для каналов: -100xxxxxxxxxx
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# ID канала Discord для чтения сообщений
# Правый клик на канале -> Копировать ID
DISCORD_CHANNEL_ID=your_discord_channel_id_here
